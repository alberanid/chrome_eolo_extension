{
  "name": "NGI Eolo 10 quotas",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Display NGI Eolo 10 quotas and alert the user when they are running out.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://care.ngi.it/"
  ]
}
